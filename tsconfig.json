{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./dist/out-tsc",
        "allowJs": false,
        "sourceMap": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "target": "es2015",
        "typeRoots": [ "node_modules/@types" ],
        "lib": [ "es2017", "dom" ]
    }
}
